AAA,QAAA,IAAA,EAAA;AAAA,OAJA,CAbA;AAmBA,MAAA,cAAA,EAAA,KAnBA;AAoBA,MAAA,MAAA,EAAA,EApBA;AAqBA,MAAA,cAAA,EAAA;AArBA,KAAA;AAwBA,GA/BA;AAgCA,EAAA,SAhCA,uBAgCA;AACA,SAAA,MAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,EAAA;AACA,SAAA,QAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA;AACA,SAAA,MAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,MAAA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,KAAA,MAAA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,KAAA,MAAA;AACA,GAvCA;AAwCA,EAAA,OAxCA,qBAwCA,CAAA,CAxCA;AAyCA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,WAAA,OAAA,CAAA,IAAA;AACA,KAHA;AAIA,IAAA,WAJA,yBAIA,CAAA,CAJA;AAKA,IAAA,UALA,wBAKA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,KAPA;AAQA,IAAA,QARA,oBAQA,MARA,EAQA;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA;AACA,WAAA,UAAA,GAAA,MAAA,CAAA,IAAA;AACA,KAXA;AAYA,IAAA,UAZA,wBAYA;AACA,WAAA,cAAA,GAAA,IAAA;AACA,KAdA;AAeA,IAAA,mBAfA,+BAeA,MAfA,EAeA,IAfA,EAeA;AACA,UAAA,IAAA,GAAA,IAAA;;AACA,UAAA,MAAA,KAAA,SAAA,EAAA;AACA,YAAA,IAAA,CAAA,MAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,cAAA,GAAA,KAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,cAAA,cAAA,GAAA,EAAA;AACA,UAAA,cAAA,GAAA,IAAA,CAAA,MAAA,CAAA,IAAA,CAAA,GAAA,CAAA;AACA,UAAA,IAAA,CAAA,MAAA,GAAA,EAAA;AACA,UAAA,IAAA,CAAA,cAAA,GAAA,cAAA;AACA,UAAA,IAAA;AACA,SARA,MAQA;AACA,UAAA,KAAA,CAAA,SAAA,CAAA;AACA,UAAA,IAAA,CAAA,KAAA,CAAA;AACA;AACA,OAbA,MAaA,IAAA,MAAA,KAAA,QAAA,EAAA;AACA,aAAA,cAAA,GAAA,KAAA;AACA,aAAA,MAAA,GAAA,EAAA;AACA,QAAA,IAAA,GAHA,CAGA;AACA;AAEA;AApCA;AAzCA,CAAA","sourcesContent":["<template>\n  <div class=\"child-section\">\n    <van-nav-bar\n      title=\"修改VIP上门预约信息\"\n      left-text=\"返回\"\n      left-arrow\n      @click-left=\"onClickLeft\"\n    />\n    <div class=\"main\">\n      <div class=\"search-list-role\">\n        <div class=\"dis_setting_i\">\n          <div class=\"s_center_t_item\">修改状态</div>\n          <van-popover\n            v-model=\"showPopover\"\n            trigger=\"click\"\n            :actions=\"actions\"\n            @select=\"onSelect\"\n            placement=\"bottom-end\"\n          >\n            <template #reference>\n              <div class=\"s_setting_t_item\">\n                <span style=\"color: #ec1f2d\">{{ statustext }}</span>\n                <van-icon name=\"arrow\" />\n              </div>\n            </template>\n          </van-popover>\n        </div>\n      </div>\n\n      <div class=\"dis_setting_icon\">\n        <div class=\"s_setting_t\" style=\"color: #e27a62\">\n          <van-icon\n            name=\"info-o\"\n            size=\"20\"\n            style=\"margin-right: 4px; color: #e27a62\"\n          />\n        </div>\n        <div class=\"s_center_t\" style=\"color: #e27a62\">\n          只有组长和负责护士可修改状态，若当前为【等待确认】，请确认信息后修改为【待采样】，否则流程将无法继续。\n        </div>\n      </div>\n\n      <div class=\"search-list-role\">\n        <div class=\"dis_setting_i\">\n          <div class=\"s_center_t_item\">负责护士</div>\n          <div class=\"s_setting_t_item\" @click=\"clickHushi\">\n            <span style=\"color: #e06596\">{{resultCheckbox}}</span>\n            <van-icon name=\"arrow\" />\n          </div>\n        </div>\n      </div>\n\n      <div class=\"search-list-role\">\n        <div class=\"dis_setting_i_mar\">\n          <div class=\"s_center_t_item_g\" style=\"width: 20%\">备注</div>\n          <van-field\n              v-model=\"remarks\"\n              rows=\"1\"\n              autosize\n              label=\"\"\n              type=\"textarea\"\n              maxlength=\"200\"\n              placeholder=\"请写下客户购买的渠道，如美团/京东等\"\n            />\n          <!-- <div class=\"select-item input-item\">\n            <input\n              v-model=\"remarks\"\n              type=\"text\"\n              name=\"remarks\"\n              placeholder=\"请写下客户购买的渠道，如美团/京东等\"\n            />\n          </div> -->\n        </div>\n      </div>\n\n      <div class=\"search-list-role-from\">\n        <div class=\"search-list-role-from-a\">\n          <div class=\"search-list-role-from-b\">上门地址</div>\n          <div class=\"search-list-role-from-c\">\n            <div class=\"s_center_t_item\" style=\"width: 20%\">姓名</div>\n            <div class=\"select-item input-item\">\n              <input\n                v-model=\"vip_person\"\n                type=\"text\"\n                name=\"vip_person\"\n                placeholder=\"请填写联系人姓名\"\n                maxlength=\"50\"\n              />\n            </div>\n          </div>\n\n          <div class=\"search-list-role-from-c\">\n            <div class=\"s_center_t_item\" style=\"width: 20%\">手机号</div>\n            <div class=\"select-item input-item\">\n              <input\n                v-model=\"vip_phone\"\n                type=\"number\"\n                name=\"vip_phone\"\n                placeholder=\"请输入联系人手机号\"\n                maxlength=\"11\"\n              />\n              <div class=\"clear_1\" @click=\"clearPhone\" v-if=\"vip_phone\">\n                <img class=\"icon_clear\" src=\"../assets/images/deleteTest.png\" />\n              </div>\n            </div>\n          </div>\n          <div class=\"search-list-role-from-c\">\n            <div class=\"s_center_t_item\" style=\"width: 20%\">所在地区</div>\n            <div class=\"select-item input-item\">\n              <input\n                v-model=\"area\"\n                type=\"text\"\n                name=\"area\"\n                placeholder=\"请选择所在地区\"\n                disabled=\"true\"\n              />\n              <div class=\"clear_1\" style=\"padding: 0px\">\n                <van-icon name=\"arrow\" />\n              </div>\n            </div>\n          </div>\n          <div class=\"search-list-role-from-g\" style=\"border-bottom: none\">\n            <div class=\"s_center_t_item_g\" >详细地址</div>\n            <!-- <el-input\n  type=\"textarea\"\n  autosize\n  placeholder=\"请输入内容\"\n  v-model=\"address\">\n</el-input> -->\n            <van-field\n              v-model=\"address\"\n              rows=\"1\"\n              autosize\n              label=\"\"\n              type=\"textarea\"\n              placeholder=\"请填写详细地址\"\n            />\n            <!-- <div class=\"select-item input-item\">\n                    <input\n                      v-model=\"vip_phone\"\n                      type=\"textarea\"\n                      name=\"vip_phone\"\n                      placeholder=\"请填写详细地址\"\n                      maxlength=\"50\"\n                    />\n                  </div> -->\n          </div>\n        </div>\n      </div>\n\n      <div class=\"search-list-role-from\">\n        <div class=\"search-list-role-from-a\">\n          <div class=\"search-list-role-from-d\">\n            <div class=\"s_center_t_item\">期望上门时间</div>\n            <div class=\"s_setting_t_item\">\n              <span>1月20日 20:00</span><van-icon name=\"arrow\" />\n            </div>\n          </div>\n          <div class=\"search-list-role-from-d\">\n            <div class=\"s_center_t_item\">特殊时间要求</div>\n            <div class=\"s_setting_t_item\">\n              <span>请选择</span><van-icon name=\"arrow\" />\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"search-list-role\">\n        <div class=\"dis_setting_i\">\n          <div class=\"s_center_t_item\">受检人数</div>\n          <div class=\"s_setting_t_item\" style=\"color: #e06596\">\n            <van-stepper\n              v-model=\"value\"\n              integer\n              input-width=\"40\"\n              theme=\"round\"\n              button-size=\"30\"\n            />\n          </div>\n        </div>\n      </div>\n\n      <div class=\"empty_view\"></div>\n\n      <div class=\"view_bottom\">\n        <div class=\"view_bottom_left\" @click=\"clickSubmit\">保存</div>\n      </div>\n    </div>\n\n    <van-dialog\n      v-model=\"isShowCheckbox\"\n      showCancelButton\n      :beforeClose=\"beforeCloseCheckbox\"\n      confirmButtonColor='#E06596'\n    >\n    <div class=\"dialog_item\">\n        <van-checkbox-group v-model=\"result\"  direction=\"horizontal\">\n          <div v-for=\"(item,index) in actions\" :key=\"index\" class=\"checkbox_class\">\n          <van-checkbox :name=\"item.text\">{{item.text}}</van-checkbox>\n          </div>\n        </van-checkbox-group>\n    </div>\n    </van-dialog>\n\n  </div>\n</template>\n\n<script>\nimport {\n  Notify,\n  Toast,\n  List,\n  Button,\n  Dialog,\n  NavBar,\n  DropdownMenu,\n  DropdownItem,\n} from \"vant\";\nexport default {\n  name: \"\",\n  components: {\n    [Dialog.Component.name]: Dialog.Component, //Dialog.Component写成这样才生效\n    [Button.name]: Button,\n  },\n  data() {\n    return {\n      roleId: \"\",\n      roleName: \"\",\n      userId: \"\",\n      value: 1,\n      remarks: \"\",\n      vip_person: \"\",\n      vip_phone: \"\",\n      area: \"\",\n      address: \"\",\n      statustext: \"等待确认\",\n      showPopover: false,\n      // 通过 actions 属性来定义菜单选项\n      actions: [\n        { text: \"等待确认\" },\n        { text: \"待采样\" },\n        { text: \"已完成\" },\n        { text: \"客户缺席\" },\n      ],\n      isShowCheckbox: false,\n      result: [],\n      resultCheckbox: \"请选择\",\n\n    };\n  },\n  activated() {\n    this.roleId = this.$route.query.id;\n    this.roleName = this.$route.query.name;\n    this.userId = this.$route.query.userId;\n    console.log(this.roleId);\n    console.log(this.roleName);\n    console.log(this.userId);\n  },\n  mounted() {},\n  methods: {\n    onClickLeft() {\n      this.$router.back();\n    },\n    clickSubmit() {},\n    clearPhone() {\n      this.vip_phone = \"\";\n    },\n    onSelect(action) {\n      Toast(action.text);\n      this.statustext = action.text;\n    },\n    clickHushi(){\n      this.isShowCheckbox = true;\n    },\n    beforeCloseCheckbox(action, done) {\n      let that = this;\n      if(action === 'confirm') {\n        if(that.result.length > 0){\n          this.isShowCheckbox = false;\n          console.log(that.result)\n          let resultCheckbox = '';\n          resultCheckbox= that.result.join(\",\");\n          that.result=[];\n          that.resultCheckbox = resultCheckbox;\n          done()\n        }else{\n          Toast('请选择负责护士');\n          done(false)\n        }\n      } else if(action === 'cancel') {\n        this.isShowCheckbox = false;\n        this.result=[];\n        done() //关闭\n      }\n      \n    },\n  },\n};\n</script>\n<style scoped lang=\"scss\">\n.child-section {\n  background-color: #f5f5f5;\n  // height: 100%;\n}\n.icon_logo {\n  width: 40px;\n  height: 40px;\n}\n\n.main {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n\n  // padding-top: 30px;\n}\n\n.dis_setting {\n  width: 100%;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 80px 50px 20px 50px;\n}\n.s_center_t {\n  font-size: 20px;\n}\n\n.dis_setting_i {\n  width: 100%;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 30px 20px;\n  background: #ffffff;\n  border-radius: 20px;\n  margin-top: 20px;\n}\n\n.dis_setting_i_mar {\n  width: 100%;\n  display: flex;\n  justify-content: space-between;\n  // align-items: center;\n  padding: 10px 20px;\n  background: #ffffff;\n  border-radius: 20px;\n  margin-top: 20px;\n}\n\n.dis_setting_icon {\n  width: 100%;\n  display: flex;\n  padding: 20px 30px 0px;\n}\n\n.s_center_t_item {\n  font-size: 32px;\n}\n\n.s_setting_t_item {\n  font-size: 32px;\n}\n\n.select-item {\n  width: 80%;\n}\n.input-item {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  position: relative;\n\n  input {\n    width: 100%;\n    height: 80px;\n    background: #ffffff;\n    border-radius: 12px;\n    border: none;\n    outline: none;\n    font-size: 28px;\n    font-family: PingFangSC-Regular, PingFang SC;\n    font-weight: 400;\n    color: #333333;\n    padding-left: 20px;\n  }\n\n  .icon_scan {\n    width: 40px;\n    height: 40px;\n  }\n}\n\n.search-list-role {\n  // height: 1110px;\n  border: 0 none;\n  // background-color: #FFFFFF;\n  width: 100%;\n  padding: 0px 30px;\n}\n\n.empty_view {\n  height: 150px;\n  width: 100%;\n}\n\n.view_bottom {\n  display: flex;\n  justify-content: space-evenly;\n  align-items: center;\n  width: 100%;\n  // position: fixed;\n  // bottom: 50px;\n  z-index: 9;\n  height: 120px;\n}\n\n.view_bottom_left {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  width: 690px;\n  height: 98px;\n  background: #e06596;\n  border-radius: 50px;\n  border: 1px solid #e06596;\n  font-size: 34px;\n  color: #ffffff;\n}\n\n.search-list-role-from {\n  // height: 1110px;\n  border: 0 none;\n  width: 100%;\n  padding: 0px 30px;\n  margin-top: 20px;\n  border-radius: 20px;\n\n  .search-list-role-from-a {\n    width: 100%;\n    display: flex;\n    flex-direction: column;\n    padding: 10px 20px;\n    background: #ffffff;\n    border-radius: 20px;\n\n    .search-list-role-from-b {\n      padding-top: 10px;\n      font-size: 32px;\n    }\n\n    .search-list-role-from-c {\n      width: 100%;\n      display: flex;\n      justify-content: space-between;\n      align-items: center;\n      background: #ffffff;\n      padding: 10px 0px;\n      border-bottom: 1px solid #dddddd;\n    }\n\n    .search-list-role-from-g{\n      width: 100%;\n      display: flex;\n      justify-content: space-between;\n      background: #ffffff;\n      padding: 10px 0px;\n      border-bottom: 1px solid #dddddd;\n\n      \n    }\n  }\n}\n\n.s_center_t_item_g{\n        width: 20%;\n        margin-top:10px;\n        font-size: 32px;\n      }\n\n.clear_1 {\n  position: absolute;\n  padding: 16px;\n  right: 0px;\n  font-size: 32px;\n}\n\n.icon_clear {\n  width: 32px;\n  height: 32px;\n}\n\n.el-textarea {\n  width: 78%;\n  border: none;\n}\n.el-textarea__inner {\n  border: none;\n}\n.van-cell {\n  padding: 6px 0px !important;\n  width: 77% !important;\n}\n\n::v-deep .van-field__control::-webkit-input-placeholder {\n  color: #666666;\n  font-size: 0.38rem;\n}\n\n.search-list-role-from-d {\n  width: 100%;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  background: #ffffff;\n  padding: 20px 0px;\n}\n\n.dialog_item{\n      display: flex;\n      // align-items: center;\n      // justify-content: center;\n      flex-direction: column;\n      padding: 20px;\n    }\n\n.checkbox_class{\n  padding: 20px;\n  display: flex;\n  align-items: center;\n  width: 50%;\n}\n</style>"],"sourceRoot":"src/pages"}]}