A,CAAA,YAAA,GAAA,KAAA;AACA,QAAA,IAAA,CAAA,IAAA,GAAA,CAAA;AACA,QAAA,IAAA,CAAA,cAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,aAAA;AACA,QAAA,IAAA;AACA,OANA,MAMA,IAAA,MAAA,KAAA,QAAA,EAAA;AACA,QAAA,IAAA,CAAA,YAAA,GAAA,KAAA;AACA,QAAA,IAAA,GAFA,CAEA;AACA;AAEA,KA1KA;AA2KA,IAAA,SA3KA,uBA2KA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,KA7KA;AA8KA,IAAA,QA9KA,sBA8KA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,KAhLA;AAiLA,IAAA,WAjLA,yBAiLA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,KAnLA;AAoLA,IAAA,aApLA,2BAoLA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,aADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA,GAAA;AAAA,UAAA,IAAA,EAAA,IAAA;AAAA,UAAA,MAAA,EAAA;AAAA;AAFA,OAAA;AAIA;AAzLA;AA/EA,CAAA","sourcesContent":["<template>\n  <div class=\"child-section\">\n    <!-- <Header>\n      <div class=\"header-content\">\n        <div class=\"header-title\">采样管收集管理</div>\n        <div class=\"header-back\">\n          <div class=\"back\" @click=\"handleTitleBack\">\n            <i class=\"iconfont icon-arrowthin\" style=\"color: #999999;\"></i>\n          </div>\n        </div>\n      </div>\n    </Header> -->\n    <van-nav-bar\n      title=\"上门预约管理台\"\n      left-text=\"返回\"\n      left-arrow\n      @click-left=\"onClickLeft\"\n    />\n    <div class=\"main\">\n      <!-- <div class=\"item_center\" @click.stop=\"clickPDF\">样本箱转运系统使用说明</div> -->\n\n      <div class=\"dis_item_center\">\n        <div class=\"s_center\">\n          <div class=\"s_center_t\">全部上门类型</div>\n          <i class=\"el-icon-caret-bottom\"></i>\n        </div>\n        <div class=\"s_center\">\n          <div class=\"s_center_t\">全部日期</div>\n          <i class=\"el-icon-caret-bottom\"></i>\n        </div>\n        <div class=\"s_center\">\n          <div class=\"s_center_t\">时间倒序</div>\n          <i class=\"el-icon-caret-bottom\"></i>\n        </div>\n      </div>\n\n      <div class=\"t_center\">\n        <div class=\"s_center_t\">筛选护士：</div>\n        <div class=\"s_center_t_r\" >\n          <div class=\"s_center_t_r_v\">\n              <div :class=\"dataIndex == index ? 's_center_t_r_i':'s_center_t_r_i_default'\" v-for=\"(item, index) in dataList\"\n            :key=\"index\" @click=\"clickTRI(item,index)\">{{item.text}}</div>\n          </div>\n        </div>\n      </div>\n      <div class=\"t_center\">\n        <div class=\"s_center_t\">筛选状态：</div>\n        <div class=\"s_center_t_r\" >\n          <div class=\"s_center_t_r_v\">\n              <div :class=\"statusIndex == index ? 's_center_t_r_i':'s_center_t_r_i_default'\" v-for=\"(item, index) in statusList\"\n            :key=\"index\" @click=\"clickStatus(item,index)\">{{item.text}}</div>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"dis_setting\">\n        <div class=\"s_center_t\">筛选后合计数据：8条</div>\n        <div class=\"s_setting_t\" style=\"color:#E06596;\" @click=\"handleSetting\"><van-icon name=\"setting-o\" size=\"24\" style=\"margin-right:4px;color:#E06596;\"/>修改配置</div>\n      </div>\n\n       <div class=\"search-list-role\">\n          <van-pull-refresh v-model=\"refreshing\" @refresh=\"onRefresh\">\n          <van-list\n          v-model=\"loading\"\n          :finished=\"finished\"\n          finished-text=\"没有更多了\"\n          @load=\"onDownLoad\"\n        >\n\n        <!-- { status:'2',type:'0',,id:5,url:'',}, -->\n        \n          <div\n            v-for=\"(item, index) in instrumentList\"\n            :key=\"index\">\n            <div class=\"search-result-view\">\n              <div class=\"dis_setting\" style=\"padding: 0px;\">\n                <div class=\"s_center_t_item\">{{ item.date }} {{item.date1}}</div>\n                <div class=\"s_setting_t_item\" style=\"color:#E06596;\">{{item.name}}</div>\n              </div>\n              <div class=\"search-result-view-item\" v-if=\"item.namet\">\n                <div class=\"search-result-view-left\">联系人</div>\n                <div class=\"search-result-view-right\">{{item.namet}}</div>\n              </div>\n              <div class=\"search-result-view-item\" v-if=\"item.phone\">\n                <div class=\"search-result-view-left\">手机号</div>\n                <div class=\"search-result-view-right\">{{item.phone}}</div>\n              </div>\n              <div class=\"search-result-view-item\" v-if=\"item.address\" style=\"positon:relative;\">\n                <div class=\"search-result-view-left\">上门地址</div>\n                <div class=\"search-result-view-right\" style=\"width: 63%;\">{{item.address}}</div>\n                <div class=\"pic-img-view\">\n                  <img class=\"pic-img-icon\" src=\"../assets/images/icon_position_red.png\" alt=\"\" />\n                </div>\n              </div>\n              <div class=\"search-result-view-item\" v-if=\"item.typetext\">\n                <div class=\"search-result-view-left\">检测类型</div>\n                <div class=\"search-result-view-right\">{{item.typetext}}</div>\n              </div>\n              <div class=\"search-result-view-item\" v-if=\"item.url\">\n                <div class=\"search-result-view-left\">客户录入</div>\n                <div class=\"search-result-view-right\">{{item.url}}</div>\n              </div>\n              <div class=\"search-result-view-item\" v-if=\"item.peoplenumber\">\n                <div class=\"search-result-view-left\">受检人数</div>\n                <div class=\"search-result-view-right\">{{item.peoplenumber}}</div>\n              </div>\n              <div class=\"search-result-view-item\" v-if=\"item.remark\">\n                <div class=\"search-result-view-left\">备注</div>\n                <div class=\"search-result-view-right\">{{item.remark}}</div>\n              </div>\n\n              <div class=\"dis_setting\" style=\"padding: 10px 0px 0px 0px;\">\n                <div class=\"s_center_t_item\" style=\"display:flex;\">\n                  <div>状态：</div>\n                  <div class=\"s_setting_t_item\" style=\"color:#E06596;\">{{ item.statustext }}</div>\n                </div>\n                <div class=\"s_setting_t_item\" style=\"display:flex;\">\n                  <!-- <div class=\"s_center_t_r_i\" style=\"margin-right:0px;\" @click=\"clickStatus(item,index)\">修改</div> -->\n                  <div class=\"s_center_t_r_i\"  @click=\"clickStatus(item,index)\">后台录入</div>\n                  <div class=\"s_center_t_r_i\" style=\"margin-right:0px;\" @click=\"clickDelete(item,index)\">删除</div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </van-list>\n</van-pull-refresh>\n          \n      </div>\n      <div class=\"empty_view\"></div>\n      \n\n      <div class=\"view_bottom\">\n        <div class=\"view_bottom_left\" @click=\"clickTeam\">+ 创建团体上门预约</div>\n        <div class=\"view_bottom_right\" @click=\"clickVip\">+ 创建VIP上门预约</div>\n      </div>\n    </div>\n\n    <van-dialog\n      v-model=\"isShowVip\"\n      showCancelButton\n      :beforeClose=\"beforeCloseVip\"\n      confirmButtonColor='#E06596'\n      confirm-button-text=\"创建\"\n    >\n    <div class=\"dialog_item\">\n        <div class=\"dialog_item_title\">确定要创建一条VIP上门预约数据吗？</div>\n    </div>\n    </van-dialog>\n\n    <van-dialog\n      v-model=\"isShowTeam\"\n      showCancelButton\n      :beforeClose=\"beforeCloseTeam\"\n      confirmButtonColor='#E06596'\n      confirm-button-text=\"创建\"\n    >\n    <div class=\"dialog_item\">\n        <div class=\"dialog_item_title\">确定要创建一条团体上门预约数据吗？</div>\n    </div>\n    </van-dialog>\n\n    <van-dialog\n      v-model=\"isShowDelete\"\n      showCancelButton\n      :beforeClose=\"beforeCloseDelete\"\n      confirmButtonColor='#E06596'\n    >\n    <div class=\"dialog_item\">\n        <div class=\"dialog_item_title\">确定要删除该条预约吗？</div>\n    </div>\n    </van-dialog>\n\n  </div>\n</template>\n\n<script>\nimport Header from \"../components/header.vue\";\nimport { getJSSDKHELP,getConveyList,conveyScan,getCheckedUserId } from \"../request/api\";\nimport { Notify,Toast,List,Button,Dialog,NavBar,DropdownMenu, DropdownItem } from \"vant\";\nimport wx from 'jweixin-module';\nexport default {\n  name: \"\",\n  components: {\n    Header,\n    [Dialog.Component.name]: Dialog.Component, //Dialog.Component写成这样才生效\n    [Button.name]: Button,\n  },\n  data() {\n    return {\n      valuedate: '',\n      date: '',\n      show: false,\n      value1: 0,\n      value2: 'a',\n      option1: [\n        { text: '全部商品', value: 0 },\n        { text: '新款商品', value: 1 },\n        { text: '活动商品', value: 2 },\n      ],\n      option2: [\n        { text: '默认排序', value: 'a' },\n        { text: '好评排序', value: 'b' },\n        { text: '销量排序', value: 'c' },\n      ],\n      dataList:[\n        { text: '全部', value: 'a' },\n        { text: '陈虹', value: 'b' },\n        { text: '陈虹虹', value: 'c' },\n        { text: '陈虹虹虹', value: 'd' },\n        { text: '陈虹虹虹', value: 'e' },\n      ],\n      dataIndex: 0,\n      dataText: '全部',\n      statusIndex: 0,\n      statusText: '全部',\n      statusList:[\n        { text: '全部', value: 'a' },\n        { text: '等待确认', value: 'b' },\n        { text: '待采样', value: 'c' },\n        { text: '已完成', value: 'd' },\n        { text: '客户缺席', value: 'e' },\n        { text: '未录入', value: 'f' },\n      ],\n      page: 1, //第几页\n      limit: 30, //每页限制\n      instrumentList: [\n        { date: '1月20日', date1: '未选择预约时间', status:'1',statustext:'未录入',type:'1',typetext:'团体上门（100人以上）',id:1,url:'',name:'未安排护士',namet:'',phone:'',address:'',peoplenumber:'',remark:''},\n        { date: '1月20日', date1: '未选择预约时间', status:'1',statustext:'未录入',type:'0',typetext:'VIP上门检测（5人以内）',id:2,url:'请客户点击下面链接httpxxxx，填写预约信息。',name:'未安排护士',namet:'',phone:'',address:'',peoplenumber:'',remark:''},\n        { date: '1月20日', date1: '08:00', status:'2',statustext:'等待确认',type:'0',typetext:'VIP上门检测（5人以内）',id:3,url:'',name:'护士姓名2',namet:'李四',phone:'12345678901',address:'北京丰台区马连道北里8号楼1单元205号',peoplenumber:'5人',remark:'小程序购买'},\n        { date: '1月20日', date1: '上午', status:'5',statustext:'已完成',type:'1',typetext:'团体上门（100人以上）',id:4,url:'',name:'陈虹，护士3',namet:'李四',phone:'12345678901',address:'北京丰台区马连道北里8号楼1单元205号',peoplenumber:'100人',remark:'已与对方确实，付费金额为4000元，希望8点开始检测'},\n        { date: '1月20日', date1: '21:00', status:'2',statustext:'等待确认',type:'0',typetext:'VIP上门检测（5人以内）',id:5,url:'',name:'机动护士',namet:'李四',phone:'12345678901',address:'北京丰台区马连道北里8号楼1单元205号',peoplenumber:'5人',remark:'美团购买，客户要求晚上9点上门。护士为张护士'},\n        \n       \n      ],\n      loading: false,\n      finished: false,\n      refreshing: false,\n      roleId: 1,  \n      isShowVip: false,\n      isShowTeam: false,\n      isShowDelete: false,\n      roleName:'',\n      userId: ''\n    };\n  },\n  activated() {\n    this.roleId = this.$route.query.id;\n    this.roleName = this.$route.query.name;\n    this.userId = this.$route.query.userId;\n    console.log(this.roleId)\n    console.log(this.roleName)\n    console.log(this.userId)\n    this.page = 1;\n    // this.instrumentList = [];\n    // this.getConveyList();\n  },\n  mounted() {\n    this.isWechat();\n  },\n  methods: {\n    clickTRI(e,index){\n      this.dataIndex = index;\n      this.dataText = e.text;\n    },\n    clickStatus(e,index){\n      this.statusIndex = index;\n      this.statusText = e.text;\n    },\n    formatDate(date) {\n      return `${date.getMonth() + 1}/${date.getDate()}`;\n    },\n    onConfirm(date) {\n      this.show = false;\n      this.date = this.formatDate(date);\n    },\n    onClickLeft() {\n      this.$router.back();\n    },\n    clickPDF() {\n      this.$router.push({\n        path: \"/instrumentPDF\"\n      });\n      return;\n    },\n    isWechat() {\n      const ua = window.navigator.userAgent.toLowerCase();\n      if (ua.match(/micromessenger/i) == 'micromessenger') {\n        // return true;\n        console.log('微信浏览器');\n        this.scanQRJssdk();\n      } else {\n        console.log('普通浏览器,请在手机微信浏览器打开此页面');\n        return false;\n      }\n    },\n    // 初始化sdk配置\n    async scanQRJssdk() {\n      // alert(`url链接:${window.location.href}`);\n\t  const u = navigator.userAgent;\n      const isAndroid = u.indexOf('Android') > -1 || u.indexOf('Linux') > -1; // Android\n      const isIOS = navigator.platform.indexOf('iPhone') != -1;//ios\n       let url = '';\n      if (isAndroid) {\n        url = location.href;\n      }\n      if (isIOS) {\n        url = location.href.split('#')[0]; // hash模式下,#后面的部分如果带上ios中config会不对\n      }\n      const api = [];\n      // 'qrCode','barCode'\n      api.push('qrCode');\n      api.push('barCode');\n      // alert(url);\n      const resData = await getJSSDKHELP({ url });\t// 根据接口返回appId，timestamp等数据\n      console.log('获取微信配置结果', resData);\n      if (resData) {\n        // alert(JSON.stringify(resData.data));\n        wx.config({\n          // beta: true,\n          debug: false,\n          appId: resData.data.appId,\n          timestamp: resData.data.timestamp,\n          nonceStr: resData.data.nonceStr,\n          signature: resData.data.signature,\n          jsApiList: ['checkJsApi', 'scanQRCode']\n        });\n        wx.ready(() => {\n          wx.checkJsApi({\n            jsApiList: ['scanQRCode'],\n            success(res) {\n              console.log('aaaa', res);\n            }\n          });\n        });\n        wx.error((res) => {\n          alert(`出错了：${res.errMsg}`);// 这个地方的好处就是wx.config配置错误，会弹出窗口哪里错误，然\t\t后根据微信文档查询即可。\n        });\n      }\n    },\n    handleTitleBack() {\n      this.$router.back();\n    },\n    getConveyList() {\n      if (this.refreshing) {\n          this.refreshing = false;\n        }\n\n      let that = this;\n      getConveyList({\n        page: this.page,\n        limit: this.limit,\n        userId: this.userId\n      }).then((res) => {\n        if (res.data.success) {\n          let instrumentList = that.instrumentList;\n\t\t\t\t\tinstrumentList = that.page == 1 ? res.data.msg : instrumentList.concat(res.data.msg)\n\n          that.loading = false;\n\n          if(res && res.data && res.data.msg.length > 0){\n            that.page = that.page + 1;\n          }else{\n             that.finished = true;\n          }\n          \n          that.instrumentList = instrumentList;\n        } else {\n            Toast(res.data.msg)\n        }\n      });\n    },\n    onDownLoad(){\n      console.log(this.page)\n      if(this.page > 1){\n        that.getConveyList();\n      }\n    },\n    onRefresh(){\n      // 清空列表数据\n      this.finished = false;\n      this.page = 1;\n      this.instrumentList = [];\n      // 重新加载数据\n      // 将 loading 设置为 true，表示处于加载状态\n      this.loading = true;\n      that.getConveyList();\n    },\n    beforeCloseVip(action, done) {\n      let that = this;\n      if(action === 'confirm') {\n        that.isShowVip = false;\n        that.page = 1;\n        that.instrumentList = [];\n        that.getConveyList();\n        done()\n      } else if(action === 'cancel') {\n        that.isShowVip = false;\n        done() //关闭\n      }\n    },\n    beforeCloseTeam(action, done) {\n      let that = this;\n      \n      if(action === 'confirm') {\n        that.isShowTeam = false;\n        that.page = 1;\n        that.instrumentList = [];\n        that.getConveyList();\n        done()\n      } else if(action === 'cancel') {\n        that.isShowTeam = false;\n        done() //关闭\n      }\n      \n    },\n    beforeCloseDelete(action, done) {\n      let that = this;\n      \n      if(action === 'confirm') {\n        that.isShowDelete = false;\n        that.page = 1;\n        that.instrumentList = [];\n        that.getConveyList();\n        done()\n      } else if(action === 'cancel') {\n        that.isShowDelete = false;\n        done() //关闭\n      }\n      \n    },\n    clickTeam(){\n      this.isShowTeam = true;\n    },\n    clickVip(){\n      this.isShowVip = true;\n    },\n    clickDelete(){\n      this.isShowDelete = true;\n    },\n    handleSetting(){\n      this.$router.push({\n        path: \"/vipSetting\",\n        query:{id: '1',name: '杨青',userId: '45'}\n      });\n    }\n  },\n};\n</script>\n<style scoped lang=\"scss\">\n.